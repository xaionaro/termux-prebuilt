prefix=/data/data/com.termux/files/usr
bindir=${prefix}/bin
datadir=${prefix}/share
includedir=${prefix}/include
libdir=${prefix}/lib

schemasdir=${datadir}/glib-2.0/schemas
dtdsdir=${datadir}/glib-2.0/dtds
giomoduledir=${libdir}/gio/modules
gio=/data/data/com.termux/files/usr/opt/glib/cross/bin/gio
gio_querymodules=/data/data/com.termux/files/usr/opt/glib/cross/bin/gio-querymodules
glib_compile_schemas=/data/data/com.termux/files/usr/opt/glib/cross/bin/glib-compile-schemas
glib_compile_resources=/data/data/com.termux/files/usr/opt/glib/cross/bin/glib-compile-resources
gdbus=/data/data/com.termux/files/usr/opt/glib/cross/bin/gdbus
gdbus_codegen=/data/data/com.termux/files/usr/opt/glib/cross/bin/gdbus-codegen
gresource=/data/data/com.termux/files/usr/opt/glib/cross/bin/gresource
gsettings=/data/data/com.termux/files/usr/opt/glib/cross/bin/gsettings

Name: GIO
Description: glib I/O library
Version: 2.80.5
Requires: glib-2.0, gobject-2.0
Requires.private: gmodule-no-export-2.0, zlib
Libs: -L${libdir} -lgio-2.0
Cflags: -I${includedir}
